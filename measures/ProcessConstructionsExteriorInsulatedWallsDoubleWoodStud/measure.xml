  <class_name>ProcessConstructionsExteriorInsulatedWallsDoubleWoodStud</class_name>
  <display_name>Set Residential Exterior Double Wood Stud Wall Construction</display_name>
  <description>This measure assigns a double wood stud construction to above-grade exterior walls adjacent to finished space.</description>
  <modeler_description>Calculates and assigns material layer properties of double wood stud constructions for above-grade walls between finished space and outside.</modeler_description>
  <arguments>
    <argument>
      <name>selectedstuddepth</name>
      <display_name>Stud Depth</display_name>
      <description>Depth of the studs.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2x4</default_value>
      <choices>
        <choice>
          <value>2x4</value>
          <display_name>2x4</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedgapdepth</name>
      <display_name>Gap Depth</display_name>
      <description>Depth of the gap between walls.</description>
      <type>Double</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>3.5</default_value>
    </argument>
    <argument>
      <name>selectedspacing</name>
      <display_name>Stud Spacing</display_name>
      <description>The on-center spacing between studs in a wall assembly.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>24 in o.c.</default_value>
      <choices>
        <choice>
          <value>24 in o.c.</value>
          <display_name>24 in o.c.</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedwallstaggered</name>
      <display_name>Staggered Studs</display_name>
      <description>Indicates that the double studs are aligned in a staggered fashion (as opposed to being center).</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>false</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedinstcavr</name>
      <display_name>Cavity Insulation Nominal R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>33</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
