  <class_name>ProcessConstructionsExteriorInsulatedWallsDoubleWoodStud</class_name>
  <display_name>Add/Replace Residential Double Wood Stud Walls</display_name>
  <description>This measure creates double wood stud constructions for the exterior walls adjacent to the living space.</description>
  <modeler_description>Calculates material layer properties of double wood stud constructions for the exterior walls adjacent to the living space. Finds surfaces adjacent to the living space and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Living Space</display_name>
      <description>The living space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedgypthickness</name>
      <display_name>Exterior Wall Mass: Thickness</display_name>
      <description>Gypsum layer thickness.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>userdefinedgyplayers</name>
      <display_name>Exterior Wall Mass: Num Layers</display_name>
      <description>Integer number of layers of gypsum.</description>
      <type>Double</type>
      <units>#</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>selectedstuddepth</name>
      <display_name>Double Wood Stud: Stud Depth</display_name>
      <description>Depth of the studs.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2x4</default_value>
      <choices>
        <choice>
          <value>2x4</value>
          <display_name>2x4</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedgapdepth</name>
      <display_name>Double Wood Stud: Gap Depth</display_name>
      <description>Depth of the gap between walls.</description>
      <type>Double</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>3.5</default_value>
    </argument>
    <argument>
      <name>selectedspacing</name>
      <display_name>Double Wood Stud: Stud Spacing</display_name>
      <description>The on-center spacing between studs in a wall assembly.</description>
