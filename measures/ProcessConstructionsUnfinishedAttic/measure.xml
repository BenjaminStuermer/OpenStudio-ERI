  <class_name>ProcessConstructionsUnfinishedAttic</class_name>
  <display_name>Add/Replace Residential Unfinished Attic Floor and Ceiling</display_name>
  <description>This measure creates constructions for the unfinished attic floor and ceiling.</description>
  <modeler_description>Calculates material layer properties of constructions for the unfinished attic floor and ceiling. Finds surfaces adjacent to the unfinished attic and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Living Space</display_name>
      <description>The living space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedattic</name>
      <display_name>Attic Space</display_name>
      <description>The attic space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selecteduains</name>
      <display_name>Unfinished Attic: Insulation Type</display_name>
      <description>The type of insulation.</description>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>Ceiling</default_value>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
