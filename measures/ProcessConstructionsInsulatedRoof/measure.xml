        </choice>
        <choice>
          <value>2x8</value>
          <display_name>2x8</display_name>
        </choice>
        <choice>
          <value>2x10</value>
          <display_name>2x10</display_name>
        </choice>
        <choice>
          <value>2x12</value>
          <display_name>2x12</display_name>
        </choice>
        <choice>
          <value>2x14</value>
          <display_name>2x14</display_name>
        </choice>
      </choices>
    </argument>
