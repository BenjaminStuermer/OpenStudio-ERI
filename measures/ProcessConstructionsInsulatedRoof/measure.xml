      <display_name>Finished Roof: Continuous Insulation Thickness</display_name>
      <description>Thickness of rigid insulation added to the roof.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>userdefinedrigidinsr</name>
      <display_name>Finished Roof: Continuous Insulation Nominal R-value</display_name>
      <description>The nominal R-value of the continuous insulation.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
