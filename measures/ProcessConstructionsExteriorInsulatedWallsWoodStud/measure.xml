  <class_name>ProcessConstructionsExteriorInsulatedWallsWoodStud</class_name>
  <display_name>Set Residential Exterior Wood Stud Wall Construction</display_name>
  <description>This measure assigns a wood stud constructions to above-grade exterior walls adjacent to finished space.</description>
  <modeler_description>Calculates and assigns material layer properties of wood stud constructions for above-grade walls between finished space and outside.</modeler_description>
  <arguments>
    <argument>
      <name>selectedstudsize</name>
      <display_name>Cavity Depth</display_name>
      <description>Depth of the stud cavity.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2x4</default_value>
      <choices>
        <choice>
          <value>2x4</value>
          <display_name>2x4</display_name>
        </choice>
        <choice>
          <value>2x6</value>
          <display_name>2x6</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedspacing</name>
      <display_name>Stud Spacing</display_name>
      <description>The on-center spacing between studs in a wall assembly.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>16 in o.c.</default_value>
      <choices>
        <choice>
          <value>16 in o.c.</value>
          <display_name>16 in o.c.</display_name>
        </choice>
        <choice>
          <value>24 in o.c.</value>
          <display_name>24 in o.c.</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedinstcavr</name>
      <display_name>Cavity Insulation Installed R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>13</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedinsfills</name>
      <display_name>Insulation Fills Cavity</display_name>
      <description>Specifies whether the cavity insulation completely fills the depth of the wall cavity.</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
