  <class_name>ProcessConstructionsFinishedBasement</class_name>
  <display_name>ProcessConstructionsUnfinishedBasement</display_name>
  <description>This measure creates constructions for the finished basement walls, floor, and rim joists.</description>
  <modeler_description>Calculates material layer properties of constructions for the finished basement walls, floor, and rim joists. Finds surfaces adjacent to the finished basement and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedfbsmt</name>
      <display_name>Of what space type is the finished basement?</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedfbsmtins</name>
      <display_name>Finished basement insulation type.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
        </choice>
        <choice>
          <value>Half Wall</value>
          <display_name>Half Wall</display_name>
        </choice>
        <choice>
          <value>Whole Wall</value>
          <display_name>Whole Wall</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedstudsize</name>
      <display_name>Wood stud size of wall cavity.</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>None</default_value>
      <choices>
        <choice>
          <value>None</value>
          <display_name>None</display_name>
        </choice>
        <choice>
          <value>2x4, 16 in o.c.</value>
          <display_name>2x4, 16 in o.c.</display_name>
        </choice>
        <choice>
          <value>2x6, 24 in o.c.</value>
          <display_name>2x6, 24 in o.c.</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfbsmtwallcavr</name>
      <display_name>Finished basement whole wall cavity insulation R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Insulation installation grade of wood stud wall cavity.</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedinsfills</name>
      <display_name>Cavity insulation fills the cavity?</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfbsmtwallcontth</name>
      <display_name>Finished basement wall continuous insulation thickness [in].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>userdefinedfbsmtwallcontr</name>
      <display_name>Finished basement wall continuous insulation R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>selectedfbsmtceiljoistheight</name>
      <display_name>Finished basement ceiling joist height [in].</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>9.25</value>
          <display_name>9.25</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfbsmtceilff</name>
      <display_name>Finished basement ceiling framing factor [frac].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.13</default_value>
    </argument>
    <argument>
