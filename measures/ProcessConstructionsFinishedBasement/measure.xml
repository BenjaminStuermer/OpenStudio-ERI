  <class_name>ProcessConstructionsFinishedBasement</class_name>
  <display_name>Add/Replace Residential Finished Basement Constructions</display_name>
  <description>This measure creates constructions for the finished basement walls, floor, and rim joists.</description>
  <modeler_description>Calculates material layer properties of constructions for the finished basement walls, floor, and rim joists. Finds surfaces adjacent to the finished basement and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedfbsmt</name>
      <display_name>Finished Basement Space</display_name>
      <description>The finished basement space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedfbsmtins</name>
      <display_name>Finished Basement: Insulation Type</display_name>
      <description>The type of insulation.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Whole Wall</default_value>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
        </choice>
        <choice>
          <value>Half Wall</value>
          <display_name>Half Wall</display_name>
        </choice>
        <choice>
          <value>Whole Wall</value>
          <display_name>Whole Wall</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedstudsize</name>
      <display_name>Finished Basement: Cavity Depth</display_name>
      <description>Depth of the stud cavity.</description>
      <type>Choice</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>None</default_value>
      <choices>
        <choice>
          <value>None</value>
          <display_name>None</display_name>
        </choice>
        <choice>
          <value>2x4, 16 in o.c.</value>
          <display_name>2x4, 16 in o.c.</display_name>
        </choice>
        <choice>
          <value>2x6, 24 in o.c.</value>
          <display_name>2x6, 24 in o.c.</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfbsmtwallcavr</name>
      <display_name>Finished Basement: Cavity Insulation Installed R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Finished Basement: Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedinsfills</name>
      <display_name>Finished Basement: Insulation Fills Cavity</display_name>
      <description>Specifies whether the cavity insulation completely fills the depth of the wall cavity.</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>false</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfbsmtwallcontth</name>
      <display_name>Finished Basement: Continuous Insulation Thickness</display_name>
      <description>The thickness of the continuous insulation.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2</default_value>
    </argument>
    <argument>
      <name>userdefinedfbsmtwallcontr</name>
      <display_name>Finished Basement: Continuous Insulation Nominal R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>10</default_value>
    </argument>
    <argument>
      <name>selectedfbsmtceiljoistheight</name>
      <display_name>Finished Basement: Ceiling Joist Height</display_name>
      <description>Height of the joist member.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2x10</default_value>
      <choices>
        <choice>
          <value>2x10</value>
          <display_name>2x10</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfbsmtceilff</name>
      <display_name>Finished Basement: Ceiling Framing Factor</display_name>
      <description>Fraction of ceiling that is framing.</description>
      <type>Double</type>
      <units>frac</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.13</default_value>
    </argument>
    <argument>
      <name>userdefinedfbsmtrimjoistr</name>
      <display_name>Finished Basement: Rim Joist Insulation R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>10</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassth</name>
      <display_name>Floor Mass: Thickness</display_name>
      <description>Thickness of the floor mass.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.625</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasscond</name>
      <display_name>Floor Mass: Conductivity</display_name>
      <description>Conductivity of the floor mass.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8004</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassdens</name>
      <display_name>Floor Mass: Density</display_name>
      <description>Density of the floor mass.</description>
      <type>Double</type>
      <units>lb/ft^3</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>34</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasssh</name>
      <display_name>Floor Mass: Specific Heat</display_name>
      <description>Specific heat of the floor mass.</description>
      <type>Double</type>
      <units>Btu/lb-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.29</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetr</name>
      <display_name>Carpet: Carpet Pad R-value</display_name>
      <description>The combined R-value of the carpet and the pad.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2.08</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetfrac</name>
      <display_name>Carpet: Floor Carpet Fraction</display_name>
      <description>Defines the fraction of a floor which is covered by carpet.</description>
      <type>Double</type>
      <units>frac</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8</default_value>
    </argument>
  </arguments>
