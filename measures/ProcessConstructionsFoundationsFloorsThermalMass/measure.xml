  <class_name>SetResidentialFloorThermalMass</class_name>
  <display_name>Set Residential Floor Thermal Mass</display_name>
  <description>This measure assigns floor mass to floors of finished spaces, with the exception of foundation slabs.</description>
  <modeler_description>Assigns material layer properties for floors of finished spaces that are not adjacent to the ground.</modeler_description>
  <arguments>
    <argument>
      <name>thick_in</name>
      <display_name>Thickness</display_name>
      <description>Thickness of the floor mass.</description>
      <type>Double</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.625</default_value>
    </argument>
    <argument>
      <name>cond</name>
      <display_name>Conductivity</display_name>
      <description>Conductivity of the floor mass.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8004</default_value>
    </argument>
    <argument>
      <name>dens</name>
      <display_name>Density</display_name>
      <description>Density of the floor mass.</description>
      <type>Double</type>
      <units>lb/ft^3</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>34</default_value>
    </argument>
    <argument>
      <name>specheat</name>
      <display_name>Specific Heat</display_name>
      <description>Specific heat of the floor mass.</description>
      <type>Double</type>
      <units>Btu/lb-R</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.29</default_value>
    </argument>
  </arguments>
