  <class_name>ProcessConstructionsInteriorInsulatedFloors</class_name>
  <display_name>Set Residential Interzonal Floor Construction</display_name>
  <description>This measure assigns a construction to the interzonal floors.</description>
  <modeler_description>Calculates material layer properties of insulated constructions for the interzonal floors between the living space and the garage. Finds surfaces adjacent to the living space and garage and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>userdefinedinstcavr</name>
      <display_name>Interzonal Floor: Cavity Insulation Nominal R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>19</default_value>
    </argument>
    <argument>
      <name>userdefinedfloorff</name>
      <display_name>Interzonal Floor: Framing Factor</display_name>
      <description>The fraction of a floor assembly that is comprised of structural framing.</description>
      <type>Double</type>
      <units>frac</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.13</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Interzonal Floor: Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedfloormassth</name>
      <display_name>Floor Mass: Thickness</display_name>
      <description>Thickness of the floor mass.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.625</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasscond</name>
      <display_name>Floor Mass: Conductivity</display_name>
      <description>Conductivity of the floor mass.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8004</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassdens</name>
      <display_name>Floor Mass: Density</display_name>
      <description>Density of the floor mass.</description>
      <type>Double</type>
      <units>lb/ft^3</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>34</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasssh</name>
      <display_name>Floor Mass: Specific Heat</display_name>
      <description>Specific heat of the floor mass.</description>
      <type>Double</type>
      <units>Btu/lb-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.29</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetr</name>
      <display_name>Carpet: Carpet Pad R-value</display_name>
      <description>The combined R-value of the carpet and the pad.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2.08</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetfrac</name>
      <display_name>Carpet: Floor Carpet Fraction</display_name>
      <description>Defines the fraction of a floor which is covered by carpet.</description>
      <type>Double</type>
      <units>frac</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8</default_value>
    </argument>
    <argument>
      <name>living_space_type</name>
      <display_name>Living space type</display_name>
      <description>Select the living space type</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>living space type</default_value>
      <choices>
        <choice>
          <value>living space type</value>
          <display_name>living space type</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>garage_space_type</name>
      <display_name>Garage space type</display_name>
      <description>Select the garage space type</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>garage space type</default_value>
      <choices>
        <choice>
          <value>garage space type</value>
          <display_name>garage space type</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
