  <class_name>ProcessConstructionsInteriorUninsulatedFloors</class_name>
  <display_name>Add/Replace Residential Uninsulated Floors</display_name>
  <description>This measure creates uninsulated constructions for the floors between living spaces and the floors between the living space and finished basement.</description>
  <modeler_description>Calculates material layer properties of uninsulated constructions for the floors between living spaces and the floors between the living space and finished basement. Finds surfaces adjacent to the living space and finished basement and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Living Space</display_name>
      <description>The living space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedfbsmt</name>
      <display_name>Finished Basement Space</display_name>
      <description>The finished basement space type.</description>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedgypthickness</name>
      <display_name>Ceiling Mass: Thickness</display_name>
      <description>Gypsum layer thickness.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>userdefinedgyplayers</name>
      <display_name>Ceiling Mass: Num Layers</display_name>
      <description>Integer number of layers of gypsum.</description>
      <type>Double</type>
      <units>#</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassth</name>
      <display_name>Floor Mass: Thickness</display_name>
      <description>Thickness of the floor mass.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.625</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasscond</name>
      <display_name>Floor Mass: Conductivity</display_name>
      <description>Conductivity of the floor mass.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8004</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassdens</name>
      <display_name>Floor Mass: Density</display_name>
      <description>Density of the floor mass.</description>
      <type>Double</type>
      <units>lb/ft^3</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>34</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasssh</name>
      <display_name>Floor Mass: Specific Heat</display_name>
      <description>Specific heat of the floor mass.</description>
      <type>Double</type>
      <units>Btu/lb-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.29</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetr</name>
      <display_name>Carpet: Carpet Pad R-value</display_name>
      <description>The combined R-value of the carpet and the pad.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2.08</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetfrac</name>
      <display_name>Carpet: Floor Carpet Fraction</display_name>
      <description>Defines the fraction of a floor which is covered by carpet.</description>
      <type>Double</type>
      <units>frac</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8</default_value>
    </argument>
