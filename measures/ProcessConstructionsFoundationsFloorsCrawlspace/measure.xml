  <class_name>ProcessConstructionsFoundationsFloorsCrawlspace</class_name>
  <display_name>Set Residential Foundations/Floors - Crawlspace Constructions</display_name>
  <description>This measure assigns constructions to the crawlspace ceilings, walls, and floors.</description>
  <modeler_description>Calculates and assigns material layer properties of constructions for: 1) ceilings above below-grade unfinished space, 2) walls between below-grade unfinished space and ground, and 3) floors below below-grade unfinished space. Below-grade spaces are assumed to be crawlspaces (and not basements) if the space height is less than 7 ft.</modeler_description>
  <arguments>
    <argument>
      <name>selectedcsins</name>
      <display_name>Insulation Type</display_name>
      <description>The type of insulation.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Wall</default_value>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
        </choice>
        <choice>
          <value>Wall</value>
          <display_name>Wall</display_name>
        </choice>
        <choice>
          <value>Ceiling</value>
          <display_name>Ceiling</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedcswallceilr</name>
      <display_name>Wall/Ceiling Continuous/Cavity Insulation Nominal R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>5</default_value>
    </argument>
    <argument>
      <name>selectedcsceiljoistheight</name>
      <display_name>Ceiling Joist Height</display_name>
      <description>Height of the joist member.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2x10</default_value>
      <choices>
        <choice>
          <value>2x10</value>
          <display_name>2x10</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Ceiling Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedcsceilff</name>
      <display_name>Ceiling Framing Factor</display_name>
      <description>Fraction of ceiling that is framing.</description>
      <type>Double</type>
      <units>frac</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.13</default_value>
    </argument>
  </arguments>
