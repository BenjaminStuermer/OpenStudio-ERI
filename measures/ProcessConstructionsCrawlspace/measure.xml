  <class_name>ProcessConstructionsCrawlspace</class_name>
  <display_name>Add/Replace Residential Crawlspace Constructions</display_name>
  <description>This measure creates constructions for the crawlspace ceiling, walls, floor, and rim joists.</description>
  <modeler_description>Calculates material layer properties of constructions for the crawlspace ceiling, walls, floor, and rim joists. Finds surfaces adjacent to the crawlspace and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Living Space</display_name>
      <description>The living space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedcrawlspace</name>
      <display_name>Crawlspace Space</display_name>
      <description>The crawlspace space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedcsins</name>
      <display_name>Crawlspace: Insulation Type</display_name>
      <description>The type of insulation.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Wall</default_value>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
        </choice>
        <choice>
          <value>Wall</value>
          <display_name>Wall</display_name>
        </choice>
        <choice>
          <value>Ceiling</value>
          <display_name>Ceiling</display_name>
        </choice>
      </choices>
