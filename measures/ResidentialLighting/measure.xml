      <name>selected_ltg</name>
      <display_name>Lighting Energy Consumption Option</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>Benchmark</value>
          <display_name>Benchmark</display_name>
        </choice>
        <choice>
          <value>Simple</value>
          <display_name>Simple</display_name>
        </choice>
        <choice>
