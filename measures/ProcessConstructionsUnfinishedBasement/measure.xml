  <class_name>ProcessConstructionsUnfinishedBasement</class_name>
  <display_name>Set Residential Unfinished Basement Constructions</display_name>
  <description>This measure assigns constructions to the unfinished basement ceiling, walls, floor, and rim joists.</description>
  <modeler_description>Calculates material layer properties of wood stud constructions for the exterior walls adjacent to the living space. Finds surfaces adjacent to the living space and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedufbsmtins</name>
      <display_name>Unfinished Basement: Insulation Type</display_name>
      <description>The type of insulation.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Whole Wall</default_value>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
        </choice>
        <choice>
          <value>Half Wall</value>
          <display_name>Half Wall</display_name>
        </choice>
        <choice>
          <value>Whole Wall</value>
          <display_name>Whole Wall</display_name>
        </choice>
        <choice>
          <value>Ceiling</value>
          <display_name>Ceiling</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedstudsize</name>
      <display_name>Unfinished Basement: Wall Cavity Depth</display_name>
      <description>Depth of the study cavity.</description>
      <type>Choice</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>None</default_value>
      <choices>
        <choice>
          <value>None</value>
          <display_name>None</display_name>
        </choice>
        <choice>
          <value>2x4, 16 in o.c.</value>
          <display_name>2x4, 16 in o.c.</display_name>
        </choice>
        <choice>
          <value>2x6, 24 in o.c.</value>
          <display_name>2x6, 24 in o.c.</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedufbsmtwallceilcavr</name>
      <display_name>Unfinished Basement: Wall/Ceiling Cavity Insulation Nominal R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Unfinished Basement: Wall Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
